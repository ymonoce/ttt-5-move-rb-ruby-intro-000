#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!"
# creates array with nine blank spaces indexed zero through eight
board = Array.new(9, " ")
puts "Where would you like to go?"
input = gets.strip
input_to_index(input)
move(board, input, xo = "X")
display_board (board)
